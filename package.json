{
  "name": "domcell",
  "version": "2.0.8",
  "description": "App for PCR Badges and Content",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-builder -p always",
    "package-win": "electron-packager .  --overwrite  --platform=win32 --arch=ia32 --icon=assets/icons/icon-domcell.png --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Domcell 2023 Beta 6.0\""
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "domricciardiphoto",
        "repo": "Domcell-git"
      }
    ]
  },
  "author": "Dominick Ricciardi",
  "license": "MIT",
  "devDependencies": {
    "electron": "^24.1.2",
    "electron-packager": "^17.1.1",
    "electron-winstaller": "^5.1.0"
  },
  "dependencies": {
    "composer": "^4.1.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.3.0",
    "yarn": "^1.22.19"
  }
}
